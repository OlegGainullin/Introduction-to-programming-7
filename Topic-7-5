#include <stdio.h>
#include <locale.h>
#include <stdlib.h>
#include <math.h>
int main()
{
    char *locale = setlocale(LC_ALL, "");
    float a = 0;
    float b = 0;
    float answer = 0;
    printf("Введите A: ");
    scanf("%e", &a);
    printf("Введите B: ");
    scanf("%e", &b);
    answer = -b/a;
    printf("x = %g", answer);
    return 0;
}
